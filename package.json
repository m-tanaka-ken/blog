{
  "name": "masaki-blog",
  "version": "1.0.0",
  "description": "My legendary Nuxt.js project",
  "author": "mtanaka",
  "private": true,
  "scripts": {
    "dev": "npm run build:articles && cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "build:articles": "processmd \"articles/**/*.md\" --stdout --outputDir articles/dist > articles/dist/summary.json",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "npm run build:articles && nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.4",
    "date-fns": "^1.30.1",
    "express": "^4.16.3",
    "lodash": "^4.17.11",
    "nuxt": "^2.3.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.1",
    "eslint": "^5.11.1",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-prettier": "2.6.2",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "prettier": "1.14.3",
    "processmd": "^4.4.0",
    "sass-loader": "^7.1.0"
  }
}
